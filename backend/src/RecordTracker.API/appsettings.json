{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "<DB_CONNECTION_STRING>"
  },
  "Auth": {
    "Jwt": {
      "Key": "<JWT_SECRET>",
      "ExpiryDays": 35
    },
    "Cookie": {
      "Name": "AuthToken",
      "Secure": false, // set to true in prod
      "SameSite": "Lax",
      "ExpiryDays": 35
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000"
    ]
  },
  "AllowedHosts": "*"
}
